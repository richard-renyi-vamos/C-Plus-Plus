// Human - robot test

#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char authorizedChk[20];
    int val;
    cout<<"What are you? (acceptable answers: human / robot) ";
    cin>>authorizedChk;
    val = strcmp("human", authorizedChk);
    if(val==0)
        cout<<"\nYou are welcome!";
    else
        cout<<"\nYou can't proceed!";
    cout<<endl;
    return 0;
}
